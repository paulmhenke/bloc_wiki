<h1><%= @user.name %>'s Wikis</h1>

<div class="row">
  <div class="col-md-6">
    <% @wikis.each do |wiki| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <%= link_to wiki.title, wiki %>
          </h4>
        </div>
      </div>
    <% end %>
  </div>
</div>
